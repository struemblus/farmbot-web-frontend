{
  "name": "farmbot-web-frontend",
  "version": "1.1.0",
  "description": "Farmbot web frontend.",
  "main": "dist/entry.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/farmbot/farmbot-web-frontend"
  },
  "scripts": {
    "build": "node_modules/webpack/bin/webpack.js --config tools/webpack.config.prd.js --display-error-details",
    "start": "webpack-dev-server --config tools/webpack.config.dev.js --content-base public/ --host 0.0.0.0",
    "test": "jest --coverage --no-cache"
  },
  "keywords": [
    "farmbot"
  ],
  "author": "farmbot.io",
  "license": "MIT",
  "optionalDependencies": {
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "@types/fastclick": "^1.0.28",
    "@types/handlebars": "^4.0.31",
    "@types/history": "^2.0.39",
    "@types/i18next": "^2.3.32",
    "@types/markdown-it": "0.0.1",
    "@types/mqtt": "0.0.32",
    "@types/node": "^6.0.50",
    "@types/react": "^0.14.44",
    "@types/react-dom": "^0.14.18",
    "@types/react-redux": "^4.4.32",
    "@types/react-router": "^2.0.38",
    "@types/react-select": "^1.0.37",
    "@types/redux": "^3.6.31",
    "@types/redux-thunk": "^2.1.31",
    "axios": "^0.14.0",
    "boxed_value": "^1.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "farmbot": "2.6.0",
    "fastclick": "^1.0.6",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.4.0",
    "i18next": "^3.4.3",
    "json-loader": "^0.5.4",
    "lodash": "^3.10.1",
    "markdown-it": "^8.2.1",
    "markdown-it-emoji": "^1.3.0",
    "moment": "2.15.2",
    "moxios": "^0.3.0",
    "node-sass": "^3.10.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.1",
    "react-router": "^3.0.0",
    "react-select": "^1.0.0-rc.2",
    "redux": "^3.3.1",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^4.0.2",
    "static-site-generator-webpack-plugin": "^3.1.0",
    "style-loader": "^0.13.0",
    "ts-loader": "^1.0.0",
    "tslint": "^3.15.1",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0",
    "weinre": "^2.0.0-pre-I0Z7U9OV"
  },
  "devDependencies": {
    "@types/enzyme": "^2.5.39",
    "@types/jest": "^16.0.1",
    "enzyme": "^2.6.0",
    "jest": "^17.0.3",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "ts-jest": "^17.0.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "html",
      "json"
    ]
  }
}